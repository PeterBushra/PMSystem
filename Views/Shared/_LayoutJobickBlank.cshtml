@{
    @using Microsoft.Extensions.Configuration
    @inject IConfiguration Configuration
}
<!DOCTYPE html>
<html lang="en">
<head>
    <partial name="Jobick/_Meta" />

    <title>@Configuration.GetSection("DefaultValue")["Title"]</title>

    <!-- Favicon icon -->
    <link rel="shortcut icon" type="image/png" href="~/Jobick/images/favicon.png">

    <!-- All StyleSheet -->
    <link href="~/Jobick/vendor/bootstrap-select/dist/css/bootstrap-select.min.css" rel="stylesheet">
    <!-- Globle CSS -->
    <link class="main-css" href="~/Jobick/css/style.css" rel="stylesheet">
    @RenderSection("Styles", false)
</head>
<body>
    <!--**********************************
        Content body start
    ***********************************-->
    @RenderBody()
    <!--**********************************
        Content body end
    ***********************************-->
    <!--**********************************
        Scripts
    ***********************************-->
    <!-- Required vendors -->
    <script src="~/Jobick/vendor/global/global.min.js"></script>
    <script src="~/Jobick/vendor/bootstrap-select/dist/js/bootstrap-select.min.js"></script>
    <script src="~/Jobick/js/custom.min.js"></script>
    <script src="~/Jobick/js/dlabnav-init.js"></script>
    
    
</body>
</html>